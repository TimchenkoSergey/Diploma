  "scripts": {
    "front-dev": "webpack --config ./webpack.config.front.js --display-error-details --mode development",
    "front-prod": "webpack --config ./webpack.config.front.js --mode production",
    "server-dev": "webpack --config ./webpack.config.server.js --display-error-details --mode development",
    "server-prod": "webpack --config ./webpack.config.server.js --mode production",
    "build-dev": "npm run server-dev && npm run front-dev",
    "build-prod": "npm run server-prod && npm run server-prod",
    "watch-front": "webpack --config ./webpack.config.front.js --watch --mode development",
    "start": "node ./build/index.js",
    "start-server-dev": "npm run copy-index && npm run build-dev && npm run start",
    "start-server-prod": "npm run copy-index && npm run build-prod && npm run start",
    "copy-index": "copy src\\public\\index.html build\\public\\"
  },
