  },
  "scripts": {
    "test": "echo 1",
    "front-dev": "webpack --config ./webpack/webpack.config.front.js --mode development",
    "front-prod": "webpack --config ./webpack/webpack.config.front.js --mode production",
    "server-dev": "webpack --config ./webpack/webpack.config.server.js --mode development",
    "server-prod": "webpack --config ./webpack/webpack.config.server.js --mode production",
    "build-dev": "npm run server-dev && npm run front-dev",
    "build-prod": "npm run server-prod && npm run server-prod",
    "watch-front": "webpack --config ./webpack/webpack.config.front.js --watch --mode development",
    "start": "node ./dist/index.js",
    "start-server-dev": "npm run build-dev && npm run start && npm run copy-index",
    "start-server-prod": "npm run build-prod && npm run start && npm run copy-index",
    "copy-index": "copy src\\public\\index.html dist\\public\\"
  },
  "dependencies": {
    "@types/axios": "0.14.0",
    "@types/react": "16.0.19",
    "@types/react-dom": "16.0.2",
    "@types/react-redux": "5.0.12",
    "@types/redux": "3.6.0",
    "@types/redux-actions": "2.2.3",
    "autoprefixer": "8.3.0",
    "awesome-typescript-loader": "3.2.3",
    "axios": "0.17.0",
    "css-loader": "0.28.7",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "node-sass": "4.7.2",
    "postcss-loader": "2.1.4",
    "react": "16.3.2",
    "react-dom": "15.6.1",
    "react-redux": "5.0.6",
    "redux": "4.0.0",
    "redux-actions": "2.2.1",
    "sass-loader": "7.0.1",
    "source-map-loader": "0.2.3",
    "style-loader": "0.19.0",
    "typescript": "2.5.3",
    "webpack": "4.8.1",
    "webpack-cli": "^2.1.3"
  }
}
